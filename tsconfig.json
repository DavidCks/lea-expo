{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "event-target-shim": ["./node_modules/event-target-shim"],
      "@assets/*": ["assets/*"],
      "@images/*": ["assets/images/*"],
      "@fonts/*": ["assets/fonts/*"],
      "@components/*": ["src/components/*"],
      "@urils/*": ["src/urils/*"],
      "@screens/*": ["src/screens/*"],
      "@/*": ["./*"]
    }
  }
}
