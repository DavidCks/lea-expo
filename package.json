{
  "name": "lea-expo",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo start -c --android",
    "start:prod": "npx expo start --no-dev",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "npx eas build --profile development --platform android --local --clear-cache",
    "build:preview": "npx eas build -p android --profile preview --local --clear-cache",
    "build:prod": "npx eas build -p android --profile production --local --clear-cache",
    "install:prod": "bundletool build-apks --bundle=$(ls build-*.aab | sort -V | tail -n1) --output=prod.apks --mode=universal --ks @davidcks__lea-expo.jks --ks-key-alias 6ffd20fdab7ff7916ba390a529692cef && unzip -o prod.apks universal.apk && rm prod.apks && mv -f universal.apk builds/universal.apk && adb install -r builds/universal.apk",
    "build:remote": "npx eas build --profile development --platform android",
    "macos:list-emulators": "~/Library/Android/sdk/emulator/emulator -list-avds",
    "macos:start-emulator": "~/Library/Android/sdk/emulator/emulator -avd \"$(~/Library/Android/sdk/emulator/emulator -list-avds | tail -n 1)\"",
    "adb:connect": "node adb-connect.js",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.1.6",
    "@config-plugins/react-native-webrtc": "^12.0.0",
